	<parameter name="zeID" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT z.bezeichnung, g.name AS Gruppenname, j.jahr, l.name AS Lagername FROM 
Zelt z
INNER JOIN
stGrZe st
ON z.zeid = st.zelt
INNER JOIN
Gruppe g
ON st.gruppe = g.grid
INNER JOIN
stGrLa
ON stgrla.gruppe = g.grid
INNER JOIN
lager l
ON stgrla.lager = l.laid
INNER JOIN
stLaJa
ON stLaJa.lager = l.laid
INNER JOIN
Jahr j
ON stLaJa.Jahr = j.jaid
WHERE
z.zeid = $P{zeID}]]></queryString>

