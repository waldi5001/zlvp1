	<parameter name="jaid" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT an.anrede AS anredebez, lt.typ AS typbez, lg.*
FROM jahr j INNER JOIN stlaja ON stlaja.jahr=j.jaid
INNER JOIN Lager l ON stlaja.lager = l.laid
INNER JOIN stlalo ON stlalo.lager=l.laid
INNER JOIN lagerort lo ON stlalo.lagerort=lo.loid
INNER JOIN stlolg ON lo.loid = stlolg.lagerort
INNER JOIN legenda lg ON lg.lgid = stlolg.legenda
INNER JOIN legendatyp lt ON lg.typ = lt.tyid
INNER JOIN anrede an ON lg.anrede = an.anid
WHERE j.jaid = $P{jaid}]]>
	</queryString>
